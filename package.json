{
	"name": "cpp-qt-generator",
	"publisher": "edenreich",
	"displayName": "cpp-qt-generator",
	"description": "This vscode extension helps to generate qt projects quickly",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/edenreich/vscode-cpp-qt-generator"
	},
	"icon": "images/cpp_qt.png",
	"keywords": [
		"cpp",
		"qt",
		"components"
	],
	"author": {
		"name": "Eden Reich",
		"email": "eden.reich@gmail.com"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createQtWidgetsApplication",
				"title": "C++/Qt: create a new QT widgets application"
			},
			{
				"command": "extension.createQtConsoleApplication",
				"title": "C++/Qt: create a new QT console application"
			},
			{
				"command": "extension.createQtQuickApplication",
				"title": "C++/Qt: create a new QT quick application"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run clean && npm run compile && npm run install",
		"clean": "rm -rf out/*",
		"compile": "tsc -p ./",
		"install": "cp -R src/templates out/templates",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1"
	}
}
