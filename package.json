{
	"name": "cpp-qt-generator",
	"displayName": "cpp-qt-generator",
	"description": "This extension helps to generate qt project on vscode quickly",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createQtWidgetsApplication",
				"title": "C++/Qt: create a new QT widgets application"
			},
			{
				"command": "extension.createQtConsoleApplication",
				"title": "C++/Qt: create a new QT console application"
			},
			{
				"command": "extension.createQtQuickApplication",
				"title": "C++/Qt: create a new QT quick application"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
